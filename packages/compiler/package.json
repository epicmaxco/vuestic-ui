{
  "name": "@vuestic/compiler",
  "version": "0.2.9",
  "type": "module",
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "acorn": "^8.15.0",
    "chalk": "^5.6.0",
    "inquirer": "^12.9.3",
    "shiki": "^1.10.1",
    "unplugin-vue-components": "^28.8.0"
  },
  "scripts": {
    "build": "rm -rf ./dist && yarn build:vite && yarn build:devtools:client",
    "build:vite": "tsup-node vite-plugin.ts --dts --format esm,cjs --target node20 --shims --out-dir ./dist/vite",
    "build:devtools:client": "vite build --config devtools/client/vite.config.ts --outDir ./dist/devtools/client",
    "dev": "cd ./playground && yarn dev",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "tsup": "^8.5.0",
    "vite": "^5.4.19"
  },
  "peerDependencies": {
    "@vue/compiler-core": "^3.4.21",
    "@vue/compiler-sfc": "^3.4.21",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "vue": "^3.4.21",
    "vuestic-ui": "*"
  },
  "exports": {
    "./vite": {
      "types": {
        "import": "./dist/vite/vite-plugin.d.ts",
        "require": "./dist/vite/vite-plugin.d.ts",
        "default": "./dist/vite/vite-plugin.d.ts"
      },
      "require": "./dist/vite/vite-plugin.cjs",
      "import": "./dist/vite/vite-plugin.js",
      "default": "./dist/vite/vite-plugin.js"
    },
    "./devtools": "./dist/devtools/client/vuestic-devtools.es.js"
  },
  "keywords": [
    "vuestic",
    "vue",
    "devtools",
    "vite"
  ],
  "files": [
    "dist",
    "Readme.md"
  ]
}
