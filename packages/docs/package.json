{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "yarn build:analysis --use-cache && vue-cli-service serve",
    "build": "yarn build:analysis && vue-cli-service build",
    "build:ci": "yarn build:analysis --use-cache && vue-cli-service build",
    "build:analysis": "yarn workspace sandbox build:analysis ../docs/src/page-configs/getting-started/tree-shaking",
    "lint": "vue-cli-service lint",
    "lint:style": "stylelint '**/*.{vue,html,css,scss}' --fix",
    "test:unit": "vue-cli-service test:unit",
    "generate:component": "plop --plopfile src/scripts/generators/index.ts",
    "generate:docspage": "plop --plopfile src/scripts/generators/documentation.ts",
    "generate:translation": "plop --plopfile src/scripts/generators/language.ts",
    "sync:translation": "i18n-unused sync en.json",
    "search:unused:translation": "i18n-unused display-unused",
    "search:missed:translation": "i18n-unused display-missed",
    "server:webapp": "npx http-server dist --push-state",
    "precommit": "lint-staged --concurrent=false"
  },
  "dependencies": {
    "@docsearch/js": "^3.2.1",
    "@vuestic/ag-grid-theme": "latest",
    "codesandbox": "^2.2.3",
    "core-js": "^3.12.1",
    "dedent": "^0.7.0",
    "lodash": "^4.17.21",
    "markdown-it": "^12.3.2",
    "markdown-it-attrs": "^4.0.0",
    "prismjs": "^1.27.0",
    "raw-loader": "^4.0.2",
    "register-service-worker": "^1.7.2",
    "spa-http-server": "^0.9.0",
    "tiny-emitter": "^2.1.0",
    "vue": "*",
    "vue-gtm": "^3.5.0",
    "vue-i18n": "^9.0.0-rc.6",
    "vue-meta": "^3.0.0-alpha.5",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "@vue/cli-plugin-babel": "^5.0.7",
    "@vue/cli-plugin-eslint": "^5.0.7",
    "@vue/cli-plugin-pwa": "^5.0.7",
    "@vue/cli-plugin-router": "^5.0.7",
    "@vue/cli-plugin-typescript": "^5.0.7",
    "@vue/cli-plugin-unit-jest": "^5.0.7",
    "@vue/cli-plugin-vuex": "^5.0.7",
    "@vue/cli-service": "^5.0.7",
    "@vue/cli-shared-utils": "5.0.7",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-standard": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^9",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^7.5.0",
    "i18n-unused": "^0.4.2",
    "lint-staged": "^11.1.2",
    "plop": "^2.7.0",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.4",
    "vue-auto-routing": "^1.0.1",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-cli-plugin-auto-routing": "^1.2.0",
    "vue-jest": "^5.0.0-alpha.7",
    "vue-property-decorator": "^9.1.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "vue-cli-service lint"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "stylelint '**/*.{vue,html,css,scss}' --fix"
    ],
    ".{htm,html,css,sss,less,scss}": [
      "stylelint '**/*.{vue,html,css,scss}' --fix"
    ]
  },
  "pwa": {
    "iconPaths": {
      "favicon32": "favicon-32x32.png",
      "favicon16": "favicon-16x16.png",
      "appleTouchIcon": "apple-touch-icon.png",
      "maskIcon": null,
      "msTileImage": null,
      "faviconSVG": null
    }
  }
}
