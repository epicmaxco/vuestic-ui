    "nuxt": "^2.12.1",
    "nuxt-i18n": "^6.9.1",
    "prismjs": "^1.20.0",
    "raw-loader": "^4.0.1",
    "typescript-eslint": "^0.0.1-alpha.0",
    "vue": "^2.6.11",
    "vue-epic-bus": "^0.1.5",
    "vue-instantsearch": "^2.7.0",
    "vue-loader": "^15.9.1",
